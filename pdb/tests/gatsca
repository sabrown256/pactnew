#!/bin/csh -f
#
# GATSCA - PDB gather/scatter test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Gather/Scatter Test ............"
set exe = $BinDir/tpdgs
set opt = ( -v $tst )
set io  = ftee

NoteD $Log ""
NoteD $Log "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a gatsca -e $xstatus -dt $dt -x $exe -rpf "$rpf" "$msg"

exit($status)
