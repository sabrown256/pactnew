#!/bin/csh -f
#
# FILL - ULTRA FILL test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

#--------------------------------------------------------------------------

#                           FILL TEST

#--------------------------------------------------------------------------

   NoteD $Log ""
   NoteD $Log -n "                    ULTRA FILL Test .............. "
   Note $Log ""

   flog $Log $RM ulfill.log ulfill.err

   set rpf = none

   if ("$Valgrind" == "") then
      flog $Log ($CFE $Ultra -l $TDir/ulfill.scm >& ulfill.log)
      set xstatus = $status
      flog $Log (grep -i "error" ulfill.log > ulfill.err)
   else
      ftee $Log $Valgrind $BinDir/ultra -e -r -x -q -s -l $TDir/ulfill.scm  |& \
      grep "ERROR SUMMARY:"                                                 |& \
      awk '{ print $4 }'   >&! $Tmp
      set xstatus = `cat $Tmp`
      set rpf     = $Tmp
   endif

#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

$TEST_CHECK part -a fill -e $xstatus -rpf "$rpf" none

exit($status)

