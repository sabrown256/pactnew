#!/bin/csh -f
#
# CFG.DB - write the final configuration database
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

set Me = $0
source $Me:h/pre-common

Note $WLog "----- write/cfg.db -----"
Note $WLog "Write: cfg.db"
Note $WLog ""

#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

Separator $WLog

NoteD $WLog "   Write configuration database - cfg.db"
Note $WLog ""


#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

flog $WLog ls -l ${PERDB_PATH}*

dbunset ALog
#dbunset Log
dbunset ldio
dbunset si

flog $WLog $PERDB -l save:db:

#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

source $Me:h/post-common

exit(0)

