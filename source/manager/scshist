#!/bin/csh -f
#
# SCSHIST - repository history whether GIT, CVS or SVN
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

unalias *

if (-d .git) then

   git log $argv

else if (-d CVS) then

   cvs history $argv

else if (-d .svn) then

   svn history $argv

endif

exit($status)
