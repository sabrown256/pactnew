#
# PROGRAM-FIN - define the actions to complete a configuration of PACT
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

Run {
   write/package         $Log $SYS_ScrDir
   write/cfg.db          $Log $SYS_ScrDir
   write/scconfig.h      $Log $SYS_ScrDir
   write/iso-c.h         $Log $SYS_ScrDir
}

Run {
   write/configured      $Log $SYS_ScrDir $CONFIG_FILE
   write/make-def        $Log $SYS_ScrDir
   write/make-macros     $Log $SYS_ScrDir
   write/makefile        $Log $SYS_ScrDir
   write/scdecls.h       $Log $SYS_ScrDir
   write/syntax.h        $Log $SYS_ScrDir
   write/spokes.scm      $Log $SYS_ScrDir
   write/install-pact    $Log $SYS_ScrDir
   write/acc             $Log $SYS_ScrDir
   write/aranlib         $Log $SYS_ScrDir
   write/smake           $Log $SYS_ScrDir
   write/dmake           $Log $SYS_ScrDir
}

