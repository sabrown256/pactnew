#
# PROGRAM-INIT - define PACT wide variables and give them default values
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

run analyze/default-bits
run analyze/rpath init

# PACT_CC_ refers to the Compiler Collection used to build PACT
# rather than COMPILER_ which is overloaded to the extreme
setenv PACT_CC_FAMILY    generic
setenv PACT_CC_EXE       `which cc`

# GOTCHA: this root may be different for CROSS_COMPILE
# need script to figure out root includes for given compiler
setenv RootInc  /usr/include

setenv LIB      lib
setenv MPIBin   /usr/bin
setenv MPILib   /usr/lib
setenv IncMPI   ""
setenv LibMPI   ""

setenv SUFFIXES  '.i'

BuildSharedLibs = FALSE
FilterDir       =
WARN_PATTERN    =
ERR_PATTERN     =
SchemeSpokes    = netcdf
CSpokes         =
ThreadLibExt    =

Tool LD {
   SpokeDepLib =
}

# define some groups for analysis

Group Z {
}

Group PNG {
}

Group JPEG {
}

Group X11 {
}

Group OGL {
}

Group QD {
}

Group SQL {
}

Group HDF {
}


