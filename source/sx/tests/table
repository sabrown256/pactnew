#!/bin/csh -f
#
# TABLE - SX table test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDBView Table Test ..........."
set exe = $BinDir/sx
set opt = ( $pvopt -l $FilesDir/pdbvtbl.scm )
set io  = pdbvtbl.tmp

if (!(-f test_1.out)) then
   cat >&! test_1.out << EOF
# table #1
  1  1  1  1
  1  2  3  4
  2  1  6  8
  2  2  8 12
  3  1  9 16
  3  2 12 20
# table #2
  1  1  1  1
  3  1  2  4
  6  2  1  8
  8  2  2 12
  9  3  1 16
 12  3  2 20
EOF
endif

if (!(-f test_2.out)) then
   cat >&! test_2.out << EOF
# table #1
  1  1  1  1
  1  2  3  4
  2  1  6  8
  2  2  8 12
  3  1  9 16
  3  2 12 20
# table #2
  1  1  1  1
  3  1  2  4
  6  2  1  8
  8  2  2 12
  9  3  1 16
 12  3  2 20
EOF
endif

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   flog $Log rm -f pdbvtbl.log pdbvtbl.pdb pdbvtbl.asc pdbvtbl.err pdbvtbl.tmp

   source ../../../scripts/test-run

   if ($xstatus == 0) then
      grep -i "error" pdbvtbl.tmp > pdbvtbl.err
      set Files = `find . -name "pdbvtbl.err" -size 0c -print`
      if ($#Files == 0) then
         set xstatus = 1
      else
         awk '(($1 != "%%CreationDate:") && !(($4 ~ /..:..:../) && ($5 ~ /199[0-9]/))) {print}' pdbvtbl.ps > pdbvtbl.ps.tmp
         flog $Log mv pdbvtbl.ps.tmp pdbvtbl.ps

         flog $Log $cross_fe $PSDIFF pdbvtbl.ps $FilesDir/ref/pdbvtbl.ps
         set xstatus = $status
      endif
   endif

   flog $Log touch pdbvtbl.foo
   set rpf = ( $rpf foofoo pdbvtbl.* )

$TEST_CHECK part -a table -e $xstatus -dt $dt -rpf "$rpf" none

exit($status)

