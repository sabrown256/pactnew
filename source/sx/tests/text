#!/bin/csh -f
#
# TEXT - SX PDB text test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SX PDB Text Test ............. "
set exe = $BinDir/sx
set opt = ( -p -e -q $ZeroSpace -l $FilesDir/sxpdbt.scm )
set io  = sxpdbt.res

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   flog $Log rm -f sxpdbt.log sxpdbt.ttx sxpdbt.a sxpdbt.res

   flog $Log cp $FilesDir/sxpdbg.scm .

   source ../../../scripts/test-run

   if ($xstatus == 0) then
      set Ref = $FilesDir/ref/sxpdbt
      if (-f $Ref) then
         flog $Log diff sxpdbt.res $Ref
         set xstatus = $status
      else
         flog $Log cp sxpdbt.res $Ref
      endif
   endif

   flog $Log touch sxpdbt.foo
   set rpf = ( $rpf sxpdbt.* )

$TEST_CHECK part -a text -e $xstatus -rpf "$rpf" none

exit($status)
