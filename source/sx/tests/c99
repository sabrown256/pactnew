#!/bin/csh -f
#
# C99 - C99 primitive data types PDB test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SX PDB C99 Test .............. "
set exe = $BinDir/sx
set rpf = 0
@ nerr  = 0

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   flog $Log cp $TDir/sxpdbg.scm .

   if ("$Valgrind" == "") then
      flog $Log ($SX -l $RootDir/sx/tests/sxc99.scm > sxc99.res)
      set xstatus = $status
      if ($xstatus == 0) then
         flog $Log (diff sxc99.res $RootDir/sx/tests/ref/sxc99 > sxc99.res.diff)
         set xstatus = $status
      endif
   else
      ftee $Log $Valgrind $exe -e -r -s -l $RootDir/sx/tests/sxc99.scm |& \
      grep "ERROR SUMMARY:"                                            |& \
      awk '{ print $4 }'   >&! $Tmp
      set xstatus = `cat $Tmp`
      set rpf     = ( $rpf $Tmp )
   endif

   set rpf = ( $rpf sxc99.res sxc99.res.diff )

$TEST_CHECK part -a c99 -e $xstatus -rpf "$rpf" none

exit($status)
