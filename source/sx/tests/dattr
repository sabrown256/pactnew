#!/bin/csh -f
#
# DATTR - ULTRA PDB attribute test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SX PDB Attribute Test ........ "
set exe = $BinDir/sx
set opt = ( -e -r -s $ZeroSpace -l $FilesDir/sxpdba.scm )
set io  = sxpdba.res

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   source ../../../scripts/test-run

   if ($xstatus == 0) then
      flog $Log (diff sxpdba.res $FilesDir/ref/sxpdba > sxpdba.res.diff)
      set xstatus = $status
   endif

   set rpf = ( $rpf sxpdba.res sxpdba.res.diff )

$TEST_CHECK part -a dattr -e $xstatus -rpf "$rpf" none

exit($status)
