#!/bin/csh -f
#
# UNIT - PYTHON unit tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "Unit Test ...................... "

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   flog $Log $CFE $PYTHON $RootDir/pya/tests/unittests.py
   set xstatus = $status

   flog $Log touch foo.pyc
   set rpf = ( *.pyc )

$TEST_CHECK part -a unit -e $xstatus -rpf "$rpf" none

exit($status)
