#!/bin/csh -f
#
# FIXDNRM - PDB fix denorm test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Fix Denorm Test ................ "
set exe = $BinDir/pdnrmt
set opt = ( -v $tst )
set io  = flog

NoteD $Log ""
NoteD $Log -n "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a fixdnrm -e $xstatus -x $exe -rpf "$rpf" none

exit($status)

