#!/bin/csh -f
#
# NET - PDB net test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Net Test ....................... "
set exe = $BinDir/pdntst
set opt = ( -v $tst )
set io  = ftee

NoteD $Log ""
NoteD $Log "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a net -e $xstatus -dt $dt -x $exe -rpf "$rpf" "$msg"

exit($status)

