#!/bin/csh -f
#
# ATTR - PDB attribute test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Attribute Test ................. "
set exe = $BinDir/pdatst
set opt = ( -v $tst )
set io  = ftee

NoteD $Log ""
NoteD $Log -n "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a attr -e $xstatus -dt $dt -x $exe -rpf "$rpf" none

exit($status)

