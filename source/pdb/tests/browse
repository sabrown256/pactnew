#!/bin/csh -f
#
# BROWSE - PDB browse test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set tst = $1

NoteD $LogF ""
NoteD $LogF -n "                    PDB Browse Test .................... "
Note $LogF ""

set UTime = `$TIMER -r`
if ("$Valgrind" == "") then
   $CFE $BinDir/pdlsts pdatst.db >& pdatst.lst
   set xstatus = $status
else
   ftee $LogF $Valgrind $BinDir/pdlsts pdatst.db |& \
   grep "ERROR SUMMARY:"                        |& \
   awk '{ print $4 }'   >&! $Tmp
   set xstatus = `cat $Tmp`
	 flog $LogF $RM $Tmp
endif
set ETime   = `$TIMER -b $UTime`

if (($xstatus == 254) || ($xstatus == 255)) then
   NoteD $LogF "OMITTED ($xstatus/$ETime)"
else if ($xstatus != 0) then
   NoteD $LogF ""
   NoteD $LogF "FAILED ($xstatus/$ETime)"
   set Err = 1
else
   NoteD $LogF "PASSED ($ETime)"
   flog $LogF rm -f pdatst.lst
endif

flog $LogF rm *.db? *.rs?

#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

$TEST_CHECK -a browse -e $Err part

exit($status)
