#!/bin/csh -f
#
# CHECKSUM - PDB checksum test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Checksum Test  ................. "
set exe = $BinDir/pdcsts

NoteD $Log ""
NoteD $Log "                    $msg"
Note $Log ""

set opt = ( -v $tst )
if ("$Valgrind" == "") then
   ftee $Log $CFE $exe $opt
   set xstatus = $status
else
   flog $Log $Valgrind $exe $opt
   set xstatus = $status
endif

$TEST_CHECK part -a checksum -e $xstatus -x $exe "$msg"

exit($status)

