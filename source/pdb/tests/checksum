#!/bin/csh -f
#
# CHECKSUM - PDB checksum test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Checksum Test  ................. "
set rpf = "none"
set exe = $BinDir/pdcsts

set tst = $1

NoteD $LogF ""
NoteD $LogF "                    $msg"
Note $LogF ""

if ("$Valgrind" == "") then
   ftee $LogF $CFE $exe -v $tst
   set xstatus = $status
else
   flog $LogF $Valgrind $exe -v $tst
   set xstatus = $status
endif

#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

$TEST_CHECK part -a checksum -e $xstatus -x $exe "$msg"

exit($status)

