#!/bin/csh -f
#
# TYPDEF - PDB typedef test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

unalias *

source ../../tests/common

set msg = "PDB Typedef Test ................... "
set exe = $BinDir/pdtdts

NoteD $LogF ""
NoteD $LogF -n "                    $msg"
Note $LogF ""

if ("$Valgrind" == "") then
   flog $LogF $CFE $exe -v $tst
   set xstatus = $status
else
   ftee $LogF $Valgrind $exe -v $tst           |& \
   grep "ERROR SUMMARY:"                       |& \
   awk '{ print $4 }'   >&! $Tmp
   set xstatus = `cat $Tmp`
   set rpf     = ( $rpf $Tmp )
endif

if (-f out.pdb) then
   flog $Log ls -l out.pdb
   flog $LogF ( grep -a Float out.pdb | grep -a double )
   if ($status == 0) then
      Note $LogF "Result ok"
   else
      Note $LogF "Result ng"
      flog $LogF set xstatus = 2
   endif
endif

set rpf = ( $rpf out.pdb )

$TEST_CHECK part -a typdef -e $xstatus -x $exe -rpf "$rpf" none

exit($status)
