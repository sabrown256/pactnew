OD = DefaultG

#
# Compiler/Loader Options
#
UniFlags =
CcFlags = ${ODC} -I${IncDir} ${MDInc} ${MDI_Inc}
LdFlags = -L${LibDir} ${MechDepLib}

CCAnnounce = ${CCompiler} ${ODC} ${CShared} ${CFLAGS}

#
# Files to remove in directory clean (make clean)
#
CleanFiles = *~ core #*#

#
# Headers Files
#
Hdrs = trhdf5.h ../../pdb_int.h
