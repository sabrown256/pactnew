#
# Object Files
#
Objs = trhdf5.o

ArObjs = ${TGTLib}(trhdf5.o)

${Objs} : ${Hdrs} ${APIs}
${LibDep} : ${Hdrs} ${APIs}

spoke : ${TGTLib} ${LibDep}
