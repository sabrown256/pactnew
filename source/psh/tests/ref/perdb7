-----------------------------------
1
2
----------------
1
2
c
----------------
perdb ,fmt:csh,v1,v2,v3
setenv v1 "1" ; 
setenv v2 "2" ; 
setenv v3 "c" ; 
----------------
perdb ,fmt:sh,v1,v2,v3
v1="1" ; export v1 ; 
v2="2" ; export v2 ; 
v3="c" ; export v3 ; 
----------------
perdb ,fmt:pl,v1,v2,v3
$v1 = "1"; 
$v2 = "2"; 
$v3 = "c"; 
----------------
perdb ,fmt:db,v1,v2,v3
v1=1
v2=2
v3=c
----------------
perdb ,v1,v2,v3
1
2
c
----------------
eval perdb ,fmt:csh,v1,v2,v3
v1 = 1
v2 = 2
v3 = c
-----------------------------------
done
