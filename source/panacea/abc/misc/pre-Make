#
# PRE-MAKE for MISC package
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

include ../dev/${System}/include/pre-Make.defs

TGTBin = none
TGTLib = ${DevLib}/libmisc.a

#
# Things upon which install depends (pact install)
#
DepLib     = 
InstallDep = ${TGTLib}
BinDep     = ${TGTLib}

OD = DefaultG

#
# Compiler/Loader Options
#
UniFlags =
CcFlags  = ${ODC} -I${DevInc} -I${IncDir} ${MDInc} ${MDGInc}
LdFlags  = -L${LibDir} ${LDFLAGS}

CCAnnounce = ${CCompiler} ${ODC} ${CShared} ${CFLAGS}

#
# Files to remove in directory clean (make clean)
#
CleanFiles = *~ core #*#

#
# Code Units
#

Hdrs = miscin.h

Objs = misc.o miscin.o

ArObjs = ${TGTLib}(misc.o) ${TGTLib}(miscin.o)

${Objs} : ${Hdrs}
${TGTLib} : ${Hdrs}

install : ${InstallDep}
	@cp *.h ${DevInc}

lib :
	rm -f ${TGTLib}
	pact install
