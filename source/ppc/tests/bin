#!/bin/csh -f
#
# BIN - PPC binary communication tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PPC Binary Test ................ "
set exe = $BinDir/pcbtst

set DPE = ""

# this test fails too often on DP machines so don't run there

# NOTE: remove this when the PPC binary stuff is rehosted on PDB Net
if (0) then
      if ("$DPE" != "") then
         NoteD $Log ""
         NoteD $Log -n "                    $msg"
         Note $Log ""

         (echo "hello" ; echo "end") | $CFE $exe >&! pcbtst.res
         set xstatus = $status
         if ($xstatus == 123) then
            set xstatus = 251
         endif
   
         set rpf = ( $rpf pcbtst.res )

         $TEST_CHECK part -a bin -e $xstatus -x $exe -rpf "$rpf" "$msg"
      endif
endif

exit($status)
