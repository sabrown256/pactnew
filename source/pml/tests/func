#!/bin/csh -f
#
# FUNC - PML function tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PML Function Test ......"
set exe = $BinDir/mlfnts
set opt = ""
set io  = flog

   NoteD $Log ""
   NoteD $Log  -n "                    $msg"

   flog $Log pushd $SrcDir
   flog $Log $MAKE funcs
   set xstatus = $status
   flog $Log popd

   if ($xstatus != 0) then
      NoteD $Log ""
      NoteD $Log "                         Can't build PML Function Test Program"
   else
      source ../../../scripts/test-run
   endif

$TEST_CHECK part -a func -e $xstatus -dt $dt -x $exe none

exit($status)


