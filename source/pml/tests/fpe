#!/bin/csh -f
#
# FPE - PML FPE tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PML FPE Test ........... "
set exe = $BinDir/mlfpets
set opt = ""
set io  = flog

      NoteD $Log ""
      NoteD $Log  -n "                    $msg"

      flog $Log pushd $SrcDir
      flog $Log $MAKE fpe
      set xstatus = $status
      flog $Log popd

      if ($xstatus != 0) then
         NoteD $Log ""
         NoteD $Log "                         Can't build PML FPE Test Program"
      else
         source ../../../scripts/test-run
      endif

$TEST_CHECK part -a fpe -e $xstatus -x $exe none

exit($status)

