#!/bin/csh -f
#
# SVD - PML SVD tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PML SVD Test ........... "
set exe = $BinDir/mlsvdt

   NoteD $Log ""
   NoteD $Log -n "                    $msg"

   flog $Log pushd $SrcDir
   flog $Log $MAKE svdsolv
   set xstatus = $status
   flog $Log popd

   if ($xstatus != 0) then
      NoteD $Log ""
      NoteD $Log "                         Can't build PML SVD Test Program"
   else
      if ("$Valgrind" == "") then
         flog $Log $cfe $exe
         set xstatus = $status
      else
         flog $Log $Valgrind $exe
         set xstatus = $status
      endif
   endif

$TEST_CHECK part -a svd -e $xstatus -x $exe none

exit($status)

