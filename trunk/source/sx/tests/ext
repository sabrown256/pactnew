#!/bin/csh -f
#
# EXT - SX extensions test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SX Extension Test ............ "
set exe = $BinDir/sx
set rpf = 0
@ nerr  = 0

NoteD $Log ""
NoteD $Log -n "                    $msg"
Note $Log ""

   set ExtDir = ../../applications/extensions
   if ((-d $ExtDir) && (-x $ExtDir/do-test)) then

      flog $Log pushd $ExtDir
      flog $Log ./do-test
      set xstatus = $status
      flog $Log popd

   else
      set xstatus = 0
   endif

$TEST_CHECK part -a ext -e $xstatus -rpf "$rpf" none

exit($status)
