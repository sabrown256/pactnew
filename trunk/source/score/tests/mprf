#!/bin/csh -f
#
# MPRF - test performance of SCORE MM
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SCORE MM Performance Test ....."
set exe = $BinDir/tscmpf
set opt = ( -m 1 )
set io  = ftee

NoteD $Log ""
NoteD $Log "                    $msg"

flog $Log pushd $SrcDir
flog $Log $MAKE memchk
set xstatus = $status
flog $Log popd

if ($xstatus != 0) then
   NoteD $Log "                         ERROR Building MM Performance Program"
   NoteD $Log "                    ${mgs}FAILED"
else
   source ../../../scripts/test-run
endif

$TEST_CHECK part -a mprf -e $xstatus -dt $dt -x $exe -rpf "$rpf" "$msg"

exit($status)

