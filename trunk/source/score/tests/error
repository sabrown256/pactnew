#!/bin/csh -f
#
# ERROR - test SCORE error handling
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SCORE Error Test .............."
set exe = $BinDir/tscer
set opt = ""
set io  = ftee

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   NoteD $Log ""

   flog $Log pushd $SrcDir
   flog $Log $MAKE error
   set xstatus = $status
   flog $Log popd

   source ../../../scripts/test-run

$TEST_CHECK part -a error -e $xstatus -dt $dt -x $exe -rpf "$rpf" "$msg"

exit($status)

