#!/bin/csh -f
#
# SVD - PML SVD tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PML SVD Test ........... "
set rpf = "none"
set exe = $BinDir/mlsvdt

   NoteD $Log ""
   NoteD $Log -n "                    $msg"

   set FAIL = "NO"

   flog $Log pushd $SrcDir
   flog $Log $MAKE svdsolv
   set TStatus = $status
   flog $Log popd

   if ($TStatus != 0) then
      NoteD $Log ""
      NoteD $Log "                         Can't build PML SVD Test Program"
      set xstatus = $TStatus
   else
      if ("$Valgrind" == "") then
         flog $LogF $cfe $exe
         set xstatus = $status
      else
         flog $Log $Valgrind $exe
         set xstatus = $status
      endif
   endif

#--------------------------------------------------------------------------
#--------------------------------------------------------------------------

$TEST_CHECK part -a svd -e $xstatus -x $exe none

exit($status)

