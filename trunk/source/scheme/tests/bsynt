#!/bin/csh -f
#
# BSYNT - SCHEME Basis syntax tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "SCHEME Basis Syntax Test ..... "
set exe = $BinDir/scheme
set opt = ( -e -l shmtst.m )
set io  = shmtst.res

   if (($do_syntax == 0) && (-d $RootDir/scheme/syntax/m)) then
      NoteD $Log ""
      NoteD $Log -n "                    $msg"
      Note $Log ""

      flog $Log cp $FilesDir/shmtst.m .

      source ../../../scripts/test-run

      if ($xstatus == 0) then
         diff shmtst.res $FilesDir/ref/shmtst >! shmtst.res.diff
         set xstatus = $status
      endif

$TEST_CHECK part -a bsynt -e $xstatus -rpf "$rpf" none

exit($status)
