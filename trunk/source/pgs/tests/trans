#!/bin/csh -f
#
# TRANS - PGS geometrical transformation tests
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PGS Trans Test ........."
set exe = $BinDir/gstrts
set opt = ( -A )
set io  = ftee

   NoteD $Log ""
   NoteD $Log "                    $msg"

   flog $Log pushd $SrcDir
   flog $Log $MAKE trans
   set xstatus = $status
   flog $Log popd

   if ($xstatus != 0) then
      NoteD $Log ""
      NoteD $Log "                         Can't build PGS Trans Test Program"
      NoteD $Log "                    ${msg}FAILED"
   else
      source ../../../scripts/test-run
   endif

$TEST_CHECK part -a trans -e $xstatus -dt $dt -x $exe -rpf "$rpf" "$msg"

exit($status)
