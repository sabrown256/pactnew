run write/package         $Log $ScrDir
run write/f90-interface   $Log $ScrDir
run write/scconfig.h      $Log $ScrDir

Run {
   write/configured      $Log $ScrDir $CONFIG_FILE
   write/make-def        $Log $ScrDir
   write/make-macros     $Log $ScrDir
   write/makefile        $Log $ScrDir
   write/iso-c.h         $Log $ScrDir
   write/scdecls.h       $Log $ScrDir
   write/syntax.h        $Log $ScrDir
   write/spokes.scm      $Log $ScrDir
   write/install-pact    $Log $ScrDir
   write/smake           $Log $ScrDir
}

