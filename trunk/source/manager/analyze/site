#!/bin/csh -f
#
# SITE - analyze and set site information
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

set Me = $0
source $Me:h/common

Separator $Log
Note $Log "Analyze: site"

dbini SYS_SITE     ""
dbini SYS_TYPE     ""
dbini DAI_ROOT     ""
#dbini DAS_ROOT     ""

envini DAS_ROOT     ""

if (-f $MngDir/extensions/sys-site) then
   eval `$MngDir/extensions/sys-site`
else
   eval `$ScrDir/sys-site -dc`
endif

# export the results
dbexp SYS_TYPE
dbexp SYS_SITE
dbexp DAI_ROOT
#dbexp DAS_ROOT

envexp DAS_ROOT

exit(0)

