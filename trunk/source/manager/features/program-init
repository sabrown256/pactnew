run features/default-bits
run features/rpath init

# GOTCHA: this root may be different for CROSS_COMPILE
# need script to figure out root includes for given compiler
setenv RootInc  /usr/include

setenv LIB      lib
setenv MPIBin   /usr/bin
setenv MPILib   /usr/lib
setenv IncMPI   ""
setenv LibMPI   ""

setenv SUFFIXES  '.i'

BuildSharedLibs = FALSE
FilterDir       =
WARN_PATTERN    =
ERR_PATTERN     =
SchemeSpokes    = netcdf
CSpokes         =
ThreadLibExt    =

Tool LD {
   SpokeDepLib =
}


