setenv PATH ${PATH}:/usr/local/bin

Group HSY {
   RootInc = /usr/local/include
   RootLib = /usr/local/lib
}

Group TEST {
   ADVISORY = ${TEST_ADVISORY}:pgs_axis
}

Group MDI {
   Inc = -I/usr/local/include
}

Group Cdefine {
   FREEBSD = TRUE
}


