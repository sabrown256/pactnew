#!/bin/csh -f
#
# PERF - PDB performance test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Performance Test ............... "
set exe = $BinDir/pdptst
set opt = ( -v $tst )
set io  = ftee

NoteD $Log ""
NoteD $Log "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a perf -e $xstatus -x $exe -rpf "$rpf" "$msg"

exit($status)

