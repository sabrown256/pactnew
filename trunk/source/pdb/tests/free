#!/bin/csh -f
#
# FREE - PD_free test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Free Test ...................... "
set exe = $BinDir/pdfrts
set opt = ""
set io  = flog

NoteD $Log ""
NoteD $Log -n "                    $msg"

source ../../../scripts/test-run

$TEST_CHECK part -a free -e $xstatus -x $exe -rpf "$rpf" none

exit($status)

