#!/bin/csh -f
#
# MASTER - PDB master test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB Master Test .................... "
set exe = $BinDir/pdbtst

NoteD $LogF ""
NoteD $LogF "                    $msg"
Note $LogF ""

if ("$Valgrind" == "") then
   ftee $LogF $CFE $exe -v $tst
   set xstatus = $status
else
   ftee $LogF $Valgrind $exe -v $tst           |& \
   grep "ERROR SUMMARY:"                       |& \
   awk '{ print $4 }'   >&! $Tmp
   set xstatus = `cat $Tmp`
   set rpf     = ( $rpf $Tmp )
endif

$TEST_CHECK part -a master -e $xstatus -x $exe -rpf "$rpf" "$msg"

exit($status)

