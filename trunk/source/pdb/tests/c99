#!/bin/csh -f
#
# C99 - PDB C99 type test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB C99 Test ....................... "
set exe = $BinDir/pdctst
set opt = ( -v $tst )
set io  = ftee

NoteD $Log ""
NoteD $Log "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a c99 -e $xstatus -x $exe -rpf "$rpf" "$msg"

exit($status)
