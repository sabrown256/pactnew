#!/bin/csh -f
#
# TYPDEF - PDB typedef test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

unalias *

source ../../tests/common

set msg = "PDB Typedef Test ................... "
set exe = $BinDir/pdtdts
set opt = ( -v $tst )
set io  = flog

NoteD $Log ""
NoteD $Log -n "                    $msg"
Note $Log ""

source ../../../scripts/test-run

if (-f out.pdb) then
   flog $Log ls -l out.pdb
   flog $Log ( grep -a Float out.pdb | grep -a double )
   if ($status == 0) then
      Note $Log "Result ok"
   else
      Note $Log "Result ng"
      flog $Log set xstatus = 2
   endif
endif

set rpf = ( $rpf out.pdb )

$TEST_CHECK part -a typdef -e $xstatus -dt $dt -x $exe -rpf "$rpf" none

exit($status)
