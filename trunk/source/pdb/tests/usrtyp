#!/bin/csh -f
#
# USRTYP - PDB user type test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "PDB User Type Test ................. "
set exe = $BinDir/pdasts
set opt = ( -v $tst )
set io  = ftee

NoteD $Log ""
NoteD $Log "                    $msg"
Note $Log ""

source ../../../scripts/test-run

$TEST_CHECK part -a usrtyp -e $xstatus -dt $dt -x $exe -rpf "$rpf" "$msg"

exit($status)

