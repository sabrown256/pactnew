OD = DefaultG

#
# Compiler/Loader Options
#
UniFlags =
CcFlags = ${ODC} -I${IncDir} ${MDInc} ${MDGInc}
LdFlags = -L${LibDir}

#
# Files to remove in directory clean (make clean)
#
CleanFiles = *~ core #*#

#
# Headers Files
#
Hdrs = trllf.h ../../pdb_int.h

#
# Object Files
#
Objs = trllf.o trlfw.c

ArObjs = ${TGTLib}(trllf.o) ${TGTLib}(trlfw.o)

${Objs} : ${Hdrs}
${LibDep} : ${Hdrs}

spoke : ${TGTLib} ${LibDep}
