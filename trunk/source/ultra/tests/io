#!/bin/csh -f
#
# IO - ULTRA I/O test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "ULTRA I/O Test ............... "
set exe = $BinDir/ultra
set opt = ( -e -r -x -q -s $ZeroSpace -l $FilesDir/uliot.scm )
set io  = uliot.log

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   source ../../../scripts/test-run

$TEST_CHECK part -a io -e $xstatus -rpf "$rpf" none

exit($status)
