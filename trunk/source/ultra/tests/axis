#!/bin/csh -f
#
# AXIS - ULTRA AXIS test
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

source ../../tests/common

set msg = "ULTRA AXIS Test .............. "
set exe = $BinDir/ultra

   NoteD $Log ""
   NoteD $Log -n "                    $msg"
   Note $Log ""

   flog $Log $RM ulaxs.log ulaxs.err

   set opt = ( -l $TDir/ulaxs.scm )
   if ("$Valgrind" == "") then
      flog $Log ($CFE $Ultra $opt >&! ulaxs.log)
      set xstatus = $status
      flog $Log ($CFE $PSDIFF ulaxs.ps $RefDir/ulaxs.ps >&! ulaxs.ps.diff)
   else
      ftee $Log $Valgrind $exe -e -r -x -q -s $opt     |& \
      grep "ERROR SUMMARY:"                            |& \
      awk '{ print $4 }'   >&! $Tmp
      set xstatus = `cat $Tmp`
      set prf     = ( $prf $Tmp )
   endif

   set rpf = ( $rpf ulaxs.* )

$TEST_CHECK part -a fill -e $xstatus -rpf "$rpf" none

exit($status)
