#!/bin/csh -f
#
# COMMON - support for ULTRA test scripts
#
# Source Version: 3.0
# Software Release #: LLNL-CODE-422942
#
# include "cpyright.h"
#

setenv Package  ultra
setenv Test     $0:t
source ../../../scripts/test-env

set ulopt = ( -e -r -x -q -s $ZeroSpace )

if ("$CROSS_FE" == "") then
   set cross_fe = ""
else
   set cross_fe = ( $CROSS_FE -v )
endif

