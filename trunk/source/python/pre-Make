generate:
	@./psys generate

incinstall:
	@./psys incinstall

devlib:
	@./psys devlib

link:
	@./psys link

install:
	@./psys link

sharedlib:
	@./psys sharedlib

inform :
	@./psys inform

test :
	@./psys pytest


