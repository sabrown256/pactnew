-----------------------------------
1
2
----------------
1
2
c
----------------
perdb ,fmt:csh,v1,v2,v3
set v1 = ( 1 ) ; 
set v2 = ( 2 ) ; 
set v3 = ( c ) ; 
----------------
perdb ,fmt:sh,v1,v2,v3
v1="1" ; 
v2="2" ; 
v3="c" ; 
----------------
perdb ,fmt:pl,v1,v2,v3
$v1 = "1"; 
$v2 = "2"; 
$v3 = "c"; 
----------------
perdb ,fmt:db,v1,v2,v3
v1=1
v2=2
v3=c
----------------
perdb ,v1,v2,v3
1
2
c
----------------
eval perdb ,fmt:csh,v1,v2,v3
v1 = 1
v2 = 2
v3 = c
-----------------------------------
done
